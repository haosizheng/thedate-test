{
    "name": "@thefoundation/thedate-subgraph",
    "version": "0.0.1",
    "description": "",
    "scripts": {
      "run-docker": "docker-compose up",

      "remove-local": "graph remove thefoundation/thedate-subgraph --node http://localhost:8020",

      "create-local": "graph create thefoundation/thedate-subgraph --node http://127.0.0.1:8020",
      "create": "graph create thefoundation/thedate-subgraph --node https://api.thegraph.com/deploy/",

      "prepare:mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
      "prepare:rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",

      "codegen": "graph codegen",
      "build": "graph build",  

      "deploy-local": "graph deploy thefoundation/thedate-subgraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020",
      "deploy": "graph deploy thefoundation/thedate-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
      
      "build-deploy:mainnet": "yarn prepare:mainnet; yarn codegen; yarn build; yarn deploy:mainnet; rm subgraph.yaml",
      "build-deploy:rinkeby": "yarn prepare:rinkeby; yarn codegen; yarn build; yarn deploy:rinkeby; rm subgraph.yaml"
    },
    "author": "",
    "license": "MIT",
    "devDependencies": {
      "@graphprotocol/graph-cli": "^0.20.0",
      "@graphprotocol/graph-ts": "^0.20.0",
      "mustache": "^3.1.0"
    },
    "dependencies": {}
  }
  