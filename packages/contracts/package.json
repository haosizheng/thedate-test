{
  "name": "@thefoundation/contracts",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npx hardhat coverage",
    "deploy-hardhat": "npx hardhat deploy --network hardhat --write true",
    "deploy-localhost": "npx hardhat deploy --network localhost --write true",
    "deploy-mainnet": "npx hardhat deploy --network mainnet --write true",
    "hardhat-node": "npx hardhat node --show-accounts",
    "graph-node": "docker-compose up",
    "subgraph:remove-local": "graph remove thefoundation/subgraph --node http://localhost:8020",
    "subgraph:create-local": "graph create thefoundation/subgraph --node http://localhost:8020",
    "subgraph:create": "graph create thefoundation/subgraph --node https://api.thegraph.com/deploy/",
    "subgraph:prepare-mainnet": "mustache config/mainnet.json subgraph.template.yaml > subgraph.yaml",
    "subgraph:prepare-rinkeby": "mustache config/rinkeby.json subgraph.template.yaml > subgraph.yaml",
    "subgraph:codegen": "graph codegen",
    "subgraph:build": "graph build",
    "subgraph:deploy-local": "graph deploy thefoundation/thedate-subgraph --ipfs http://localhost:5001 --node http://localhost:8020",
    "subgraph:deploy": "graph deploy thefoundation/thedate-subgraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "subgraph:build-deploy:mainnet": "yarn prepare:mainnet; yarn codegen; yarn build; yarn deploy:mainnet; rm subgraph.yaml",
    "subgraph:build-deploy:rinkeby": "yarn prepare:rinkeby; yarn codegen; yarn build; yarn deploy:rinkeby; rm subgraph.yaml"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.0.2",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.10",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.3.1",
    "hardhat": "^2.3.3",
    "hardhat-abi-exporter": "^2.2.1",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-deploy": "^0.8.11",
    "hardhat-gas-reporter": "^1.0.4",
    "mustache": "^3.1.0",
    "prettier": "^2.3.2",
    "prettier-plugin-solidity": "^1.0.0-beta.17",
    "solhint": "^3.3.6",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.7.16",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.0.0",
    "typechain": "^5.0.0",
    "typescript": "^4.3.2",
    "yarn-upgrade-all": "^0.5.4"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.2.0",
    "zokrates-js": "^1.0.34"
  }
}
